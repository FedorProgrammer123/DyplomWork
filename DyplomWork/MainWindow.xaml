<Window x:Class="DyplomWork.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DyplomWork"
        mc:Ignorable="d"
        Title="Приложение для тестирования и проверки знаний + учёта посещаемости студентов на занятиях" Height="550" Width="900" WindowStartupLocation="CenterScreen" MinWidth="900" MinHeight="550" Icon="/Resources/graduation-hat.png">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="#7dc408">
            <TextBlock Text="Приложение для тестирования и проверки знаний + учёта посещаемости студентов" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
            <Image Source="Resources/graduation-hat.png" HorizontalAlignment="Left"></Image>
        </Grid>
        <Frame x:Name="MainFrame" Grid.Row="1" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
